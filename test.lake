
-- function call
def f() {
  return 5 + 5
}
assert(f() == 10)

-- 'var'
var a = 5
assert(a == 5)
print(a)

-- while loop
var i = 10
while i < 20 {
  i = i + 1
}
assert(i == 20)

-- numeric for loop
var total = 0
for i = 1, 10 {
  total = total + i
}
assert(total == 55)

-- generic loop
var xs = {"abc", "def", "ghi"}
var x = ""
for k, v in ipairs(xs) {
  x = x .. k .. v
}
assert(x == "1abc2def3ghi")
